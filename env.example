# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/cascade
REDIS_URL=redis://localhost:6379

# Vector Database
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment
MILVUS_HOST=localhost
MILVUS_PORT=19530

# AI/ML Services
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# File Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET=cascade-files

# External Data Providers
BLOOMBERG_API_KEY=your_bloomberg_api_key
FACTSET_API_KEY=your_factset_api_key
CAPIQ_API_KEY=your_capiq_api_key
REFINITIV_API_KEY=your_refinitiv_api_key
PITCHBOOK_API_KEY=your_pitchbook_api_key

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Application Settings
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:3000

# Excel Add-in
EXCEL_ADDIN_URL=https://localhost:3001
OFFICE_JS_URL=https://appsforoffice.microsoft.com/lib/1/hosted/office.js

# Web Scraping
PLAYWRIGHT_BROWSER_PATH=/usr/bin/chromium-browser
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/cascade.log
